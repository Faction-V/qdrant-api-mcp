{
  "name": "qdrant-api-mcp",
  "version": "1.0.0",
  "description": "Model Context Protocol server for Qdrant Collections & Points APIs",
  "main": "dist/index.js",
  "bin": {
    "qdrant-api-mcp": "dist/mcp-server.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "qdrant-mcp-config.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordotech/qdrant-api-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/jordotech/qdrant-api-mcp/issues"
  },
  "homepage": "https://github.com/jordotech/qdrant-api-mcp#readme",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "node dist/index.js &",
    "start:mcp": "node dist/mcp-server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register src/index.ts'",
    "dev:mcp": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register src/mcp-server.ts'",
    "generate-client": "openapi --input ./openapi-collections.ytt.yaml --output ./src/generated/qdrant-client",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "qdrant",
    "mcp",
    "model-context-protocol",
    "vector"
  ],
  "author": "jordotech",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.3",
    "openapi-typescript-codegen": "^0.29.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
