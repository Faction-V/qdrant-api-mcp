{
  "mcpServers": {
    "qdrant-api": {
      "command": "npx",
      "args": [
        "qdrant-api-mcp"
      ],
      "env": {
        "QDRANT_URL": "http://localhost:6333",
        "QDRANT_API_KEY": "",
        "PORT": "3000",
        "HOST": "localhost"
      },
      "alwaysAllow": [
        "list_collections",
        "create_collection",
        "get_collection",
        "delete_collection",
        "update_collection"
      ]
    }
  }
}
